          "helpUrl": "https://learn.microsoft.com/dotnet/aspire/diagnostics/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/microsoft/aspire.dev/main/src/frontend/src/content/docs/diagnostics/{id}.mdx",
          "indexUrl": "https://raw.githubusercontent.com/microsoft/aspire.dev/main/src/frontend/src/content/docs/diagnostics/overview.mdx"
        }
      }
    },
    "extensions": {
