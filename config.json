        "BC": {
          "description": "MSBuild BuildCheck diagnostics",
          "pattern": "^BC\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/dotnet/core/tools/buildcheck-rules/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/core/tools/buildcheck-rules/{id}.md",
