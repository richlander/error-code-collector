          "indexUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md"
        },
        "CTXOPTGEN": {
          "description": "Contextual options generator diagnostics",
          "pattern": "^CTXOPTGEN\\d{3}$",
          "helpUrl": "https://aka.ms/dotnet-extensions-warnings/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md"
        },
        "EXTEXP": {
          "description": "Extensions experimental APIs",
          "pattern": "^EXTEXP\\d{4}$",
          "helpUrl": "https://aka.ms/dotnet-extensions-warnings/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md"
        },
        "EXTOBS": {
          "description": "Extensions obsoletions",
          "pattern": "^EXTOBS\\d{4}$",
          "helpUrl": "https://aka.ms/dotnet-extensions-warnings/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/extensions/main/docs/list-of-diagnostics.md"
        }
      }
    },
    "msbuild": {
      "prefixes": {
        "MSB": {
          "description": "MSBuild errors and warnings",
          "pattern": "^MSB\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/visualstudio/msbuild/errors/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/MicrosoftDocs/visualstudio-docs/main/docs/msbuild/errors/{id}.md",
          "indexUrl": null
