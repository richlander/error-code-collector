{
  "repos": {
    "roslyn": {
      "prefixes": {
        "CS": {
          "description": "C# compiler errors and warnings",
          "pattern": "^CS\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/dotnet/csharp/language-reference/compiler-messages/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/csharp/language-reference/compiler-messages/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/csharp/language-reference/compiler-messages/index.md"
        }
      }
    },
    "runtime": {
      "prefixes": {
        "SYSLIB": {
          "description": "Runtime obsoletions, analyzers, and experimental APIs",
          "pattern": "^SYSLIB\\d{4}$",
          "helpUrl": "https://aka.ms/dotnet-warnings/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/fundamentals/syslib-diagnostics/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/runtime/main/docs/project/list-of-diagnostics.md"
        }
      }
    },
    "sdk": {
      "prefixes": {
        "NETSDK": {
          "description": "SDK build and restore errors",
          "pattern": "^NETSDK\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/dotnet/core/tools/sdk-errors/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/core/tools/sdk-errors/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/core/tools/sdk-errors/index.md"
        },
        "CA": {
          "description": "Code analysis quality rules",
          "pattern": "^CA\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/dotnet/fundamentals/code-analysis/quality-rules/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/fundamentals/code-analysis/quality-rules/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/docs/main/docs/fundamentals/code-analysis/quality-rules/index.md"
        }
      }
    },
    "aspnetcore": {
      "prefixes": {
        "ASP": {
          "description": "ASP.NET Core framework analyzers",
          "pattern": "^ASP\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/aspnet/core/diagnostics/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/diagnostics/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/diagnostics/code-analysis.md"
        },
        "BL": {
          "description": "Blazor component diagnostics",
          "pattern": "^BL\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/aspnet/core/diagnostics/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/diagnostics/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/diagnostics/code-analysis.md"
        },
        "MVC": {
          "description": "MVC analyzers",
          "pattern": "^MVC\\d{4}$",
          "helpUrl": "https://learn.microsoft.com/aspnet/core/diagnostics/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/diagnostics/{id}.md",
          "indexUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/diagnostics/code-analysis.md"
        },
        "RDG": {
          "description": "Request Delegate Generator diagnostics",
          "pattern": "^RDG\\d{3}$",
          "helpUrl": "https://learn.microsoft.com/aspnet/core/fundamentals/aot/request-delegate-generator/diagnostics/{id}",
          "markdownUrl": "https://raw.githubusercontent.com/dotnet/AspNetCore.Docs/main/aspnetcore/fundamentals/aot/request-delegate-generator/diagnostics/{id}.md",
          "indexUrl": null
